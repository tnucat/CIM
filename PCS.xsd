<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:m="http://iec.ch/TC57/2007/PCS#"
           xmlns:sawsdl="http://www.w3.org/ns/sawsdl"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://iec.ch/TC57/2007/PCS#"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">
   <xs:annotation/>
   <xs:element name="PCS" type="m:PCS"/>
   <xs:complexType name="PCS">
      <xs:sequence>
         <xs:element name="ConductingEquipment"
                     type="m:ConductingEquipment"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="EnergyConsumer"
                     type="m:EnergyConsumer"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="PowerElectronicsConnection"
                     type="m:PowerElectronicsConnection"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="RegulatingControl"
                     type="m:RegulatingControl"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="RotatingMachine"
                     type="m:RotatingMachine"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BaseVoltage"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#BaseVoltage">
      <xs:annotation>
         <xs:documentation>Defines a system base voltage which is referenced.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="ConductingEquipment"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#ConductingEquipment">
      <xs:annotation>
         <xs:documentation>The parts of the AC power system that are designed to carry current or that are conductively connected through terminals.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="BaseVoltage"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:BaseVoltage"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ConductingEquipment.BaseVoltage">
            <xs:annotation>
               <xs:documentation>Base voltage of this conducting equipment.  Use only when there is no voltage level container used and only one base voltage applies.  For example, not used for transformers.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="GroundingAction"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:GroundAction"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ConductingEquipment.GroundingAction">
            <xs:annotation>
               <xs:documentation>Action involving grounding operation on this conducting equipment.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="JumpingAction"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:JumperAction"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ConductingEquipment.JumpingAction">
            <xs:annotation>
               <xs:documentation>Jumper action involving jumping operation on this conducting equipment.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Outage"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Outage"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ConductingEquipment.Outage">
            <xs:annotation>
               <xs:documentation>The outage that is isolated by the outage isolation equipment.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="EnergyConsumer"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer">
      <xs:annotation>
         <xs:documentation>Generic user of energy - a  point of consumption on the power system model.</xs:documentation>
         <xs:documentation>EnergyConsumer.pfixed, .qfixed, .pfixedPct and .qfixedPct have meaning only if there is no LoadResponseCharacteristic associated with EnergyConsumer or if LoadResponseCharacteristic.exponentModel is set to False.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="customerCount"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:integer"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.customerCount">
            <xs:annotation>
               <xs:documentation>Number of individual customers represented by this demand.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="grounded"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:boolean"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.grounded">
            <xs:annotation>
               <xs:documentation>Used for Yn and Zn connections. True if the neutral is solidly grounded.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="p"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ActivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.p">
            <xs:annotation>
               <xs:documentation>Active power of the load. Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
               <xs:documentation>For voltage dependent loads the value is at rated voltage.</xs:documentation>
               <xs:documentation>Starting value for a steady state solution.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="pfixed"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ActivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.pfixed">
            <xs:annotation>
               <xs:documentation>Active power of the load that is a fixed quantity and does not vary as load group value varies. Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="pfixedPct"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PerCent"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.pfixedPct">
            <xs:annotation>
               <xs:documentation>Fixed active power as a percentage of load group fixed active power. Used to represent the time-varying components.  Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="phaseConnection"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PhaseShuntConnectionKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.phaseConnection">
            <xs:annotation>
               <xs:documentation>The type of phase connection, such as wye or delta.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="q"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ReactivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.q">
            <xs:annotation>
               <xs:documentation>Reactive power of the load. Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
               <xs:documentation>For voltage dependent loads the value is at rated voltage.</xs:documentation>
               <xs:documentation>Starting value for a steady state solution.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="qfixed"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ReactivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.qfixed">
            <xs:annotation>
               <xs:documentation>Reactive power of the load that is a fixed quantity and does not vary as load group value varies. Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="qfixedPct"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PerCent"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.qfixedPct">
            <xs:annotation>
               <xs:documentation>Fixed reactive power as a percentage of load group fixed reactive power. Used to represent the time-varying components.  Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="EnergyConsumerAction"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:EnergyConsumerAction"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.EnergyConsumerAction">
            <xs:annotation>
               <xs:documentation>The energy consumer action that is performed on the energy consumer</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="LoadDynamics"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:LoadDynamics"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.LoadDynamics">
            <xs:annotation>
               <xs:documentation>Load dynamics model used to describe dynamic behaviour of this energy consumer.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="LoadResponse"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:LoadResponseCharacteristic"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.LoadResponse">
            <xs:annotation>
               <xs:documentation>The load response characteristic of this load.  If missing, this load is assumed to be constant power.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PowerCutZone"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PowerCutZone"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumer.PowerCutZone">
            <xs:annotation>
               <xs:documentation>The  energy consumer is assigned to this power cut zone.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="EnergyConsumerAction"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#EnergyConsumerAction">
      <xs:annotation>
         <xs:documentation>Action to connect or disconnect the Energy Consumer from its Terminal</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="GeneratingUnit"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#GeneratingUnit">
      <xs:annotation>
         <xs:documentation>A single or set of synchronous machines for converting mechanical power into alternating-current power. For example, individual machines within a set may be defined for scheduling purposes while a single control signal is derived for the set. In this case there would be a GeneratingUnit for each member of the set and an additional GeneratingUnit corresponding to the set.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="GroundAction"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#GroundAction">
      <xs:annotation>
         <xs:documentation>Action on ground as a switching step.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="HydroPump"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#HydroPump">
      <xs:annotation>
         <xs:documentation>A synchronous motor-driven pump, typically associated with a pumped storage plant.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="JumperAction"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#JumperAction">
      <xs:annotation>
         <xs:documentation>Action on jumper as a switching step.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="LoadDynamics"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#LoadDynamics">
      <xs:annotation>
         <xs:documentation>Load whose behaviour is described by reference to a standard model &lt;font color="#0f0f0f"&gt;or by definition of a user-defined model.&lt;/font&gt;</xs:documentation>
         <xs:documentation>A standard feature of dynamic load behaviour modelling is the ability to associate the same behaviour to multiple energy consumers by means of a single load definition.  The load model is always applied to individual bus loads (energy consumers).</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="LoadResponseCharacteristic"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#LoadResponseCharacteristic">
      <xs:annotation>
         <xs:documentation>Models the characteristic response of the load demand due to changes in system conditions such as voltage and frequency. It is not related to demand response.</xs:documentation>
         <xs:documentation>If LoadResponseCharacteristic.exponentModel is True, the exponential voltage or frequency dependent models are specified and used as to calculate active and reactive power components of the load model.</xs:documentation>
         <xs:documentation>The equations to calculate active and reactive power components of the load model are internal to the power flow calculation, hence they use different quantities depending on the use case of the data exchange.</xs:documentation>
         <xs:documentation>The equations for exponential voltage dependent load model injected power are:</xs:documentation>
         <xs:documentation>pInjection= Pnominal* (Voltage/cim:BaseVoltage.nominalVoltage) ** cim:LoadResponseCharacteristic.pVoltageExponent</xs:documentation>
         <xs:documentation>qInjection= Qnominal* (Voltage/cim:BaseVoltage.nominalVoltage) ** cim:LoadResponseCharacteristic.qVoltageExponent</xs:documentation>
         <xs:documentation>Where:</xs:documentation>
         <xs:documentation>1) * means "multiply" and ** is "raised to power of";</xs:documentation>
         <xs:documentation>2) Pnominal and Qnominal represent the active power and reactive power at nominal voltage as any load described by the voltage exponential model shall be given at nominal voltage.  This means that EnergyConsumer.p and EnergyConsumer.q  are at nominal voltage.</xs:documentation>
         <xs:documentation>3) After power flow is solved:</xs:documentation>
         <xs:documentation>-pInjection and qInjection correspond to SvPowerflow.p and SvPowerflow.q respectively.</xs:documentation>
         <xs:documentation>- Voltage corresponds to SvVoltage.v at the TopologicalNode where the load is connected.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="Outage" sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage">
      <xs:annotation>
         <xs:documentation>Document describing details of an active or planned outage in a part of the electrical network.</xs:documentation>
         <xs:documentation>A non-planned outage may be created upon:</xs:documentation>
         <xs:documentation>- a breaker trip,</xs:documentation>
         <xs:documentation>- a fault indicator status change,</xs:documentation>
         <xs:documentation>- a meter event indicating customer outage,</xs:documentation>
         <xs:documentation>- a reception of one or more customer trouble calls, or</xs:documentation>
         <xs:documentation>- an operator command, reflecting information obtained from the field crew.</xs:documentation>
         <xs:documentation>Outage restoration may be performed using a switching plan which complements the outage information with detailed switching activities, including the relationship to the crew and work.</xs:documentation>
         <xs:documentation>A planned outage may be created upon:</xs:documentation>
         <xs:documentation>- a request for service, maintenance or construction work in the field, or</xs:documentation>
         <xs:documentation>- an operator-defined outage for what-if/contingency network analysis.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:simpleType name="PhaseCode"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#PhaseCode">
      <xs:annotation>
         <xs:documentation>An unordered enumeration of phase identifiers.  Allows designation of phases for both transmission and distribution equipment, circuits and loads.   The enumeration, by itself, does not describe how the phases are connected together or connected to ground.  Ground is not explicitly denoted as a phase.</xs:documentation>
         <xs:documentation>Residential and small commercial loads are often served from single-phase, or split-phase, secondary circuits. For the example of s12N, phases 1 and 2 refer to hot wires that are 180 degrees out of phase, while N refers to the neutral wire. Through single-phase transformer connections, these secondary circuits may be served from one or two of the primary phases A, B, and C. For three-phase loads, use the A, B, C phase codes instead of s12N.</xs:documentation>
         <xs:documentation>The integer values are from IEC 61968-9 to support revenue metering applications.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="A">
            <xs:annotation>
               <xs:documentation>Phase A.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AB">
            <xs:annotation>
               <xs:documentation>Phases A and B.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ABC">
            <xs:annotation>
               <xs:documentation>Phases A, B, and C.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ABCN">
            <xs:annotation>
               <xs:documentation>Phases A, B, C, and N.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ABN">
            <xs:annotation>
               <xs:documentation>Phases A, B, and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AC">
            <xs:annotation>
               <xs:documentation>Phases A and C.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ACN">
            <xs:annotation>
               <xs:documentation>Phases A, C and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AN">
            <xs:annotation>
               <xs:documentation>Phases A and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="B">
            <xs:annotation>
               <xs:documentation>Phase B.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BC">
            <xs:annotation>
               <xs:documentation>Phases B and C.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BCN">
            <xs:annotation>
               <xs:documentation>Phases B, C, and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BN">
            <xs:annotation>
               <xs:documentation>Phases B and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="C">
            <xs:annotation>
               <xs:documentation>Phase C.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CN">
            <xs:annotation>
               <xs:documentation>Phases C and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="N">
            <xs:annotation>
               <xs:documentation>Neutral phase.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="X">
            <xs:annotation>
               <xs:documentation>Unknown non-neutral phase.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="XN">
            <xs:annotation>
               <xs:documentation>Unknown non-neutral phase plus neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="XY">
            <xs:annotation>
               <xs:documentation>Two unknown non-neutral phases.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="XYN">
            <xs:annotation>
               <xs:documentation>Two unknown non-neutral phases plus neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="none">
            <xs:annotation>
               <xs:documentation>No phases specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="s1">
            <xs:annotation>
               <xs:documentation>Secondary phase 1.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="s12">
            <xs:annotation>
               <xs:documentation>Secondary phase 1 and 2.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="s12N">
            <xs:annotation>
               <xs:documentation>Secondary phases 1, 2, and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="s1N">
            <xs:annotation>
               <xs:documentation>Secondary phase 1 and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="s2">
            <xs:annotation>
               <xs:documentation>Secondary phase 2.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="s2N">
            <xs:annotation>
               <xs:documentation>Secondary phase 2 and neutral.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="PhaseShuntConnectionKind"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#PhaseShuntConnectionKind">
      <xs:annotation>
         <xs:documentation>The configuration of phase connections for a single terminal device such as a load or capacitor.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="D">
            <xs:annotation>
               <xs:documentation>Delta connection.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="G">
            <xs:annotation>
               <xs:documentation>Ground connection; use when explicit connection to ground needs to be expressed in combination with the phase code, such as for electrical wire/cable or for meters.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="I">
            <xs:annotation>
               <xs:documentation>Independent winding, for single-phase connections.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Y">
            <xs:annotation>
               <xs:documentation>Wye connection.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Yn">
            <xs:annotation>
               <xs:documentation>Wye, with neutral brought out for grounding.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="PowerCutZone"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerCutZone">
      <xs:annotation>
         <xs:documentation>An area or zone of the power system which is used for load shedding purposes.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="PowerElectronicsConnection"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection">
      <xs:annotation>
         <xs:documentation>A connection to the AC network for energy production or consumption that uses power electronics rather than rotating machines.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="maxIFault"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PU"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.maxIFault">
            <xs:annotation>
               <xs:documentation>Maximum fault current this device will contribute, in per-unit of rated current, before the converter protection will trip or bypass.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="maxQ"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ReactivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.maxQ">
            <xs:annotation>
               <xs:documentation>Maximum reactive power limit. This is the maximum (nameplate) limit for the unit.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="minQ"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ReactivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.minQ">
            <xs:annotation>
               <xs:documentation>Minimum reactive power limit for the unit. This is the minimum (nameplate) limit for the unit.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="p"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ActivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.p">
            <xs:annotation>
               <xs:documentation>Active power injection. Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
               <xs:documentation>Starting value for a steady state solution.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="q"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ReactivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.q">
            <xs:annotation>
               <xs:documentation>Reactive power injection. Load sign convention is used, i.e. positive sign means flow out from a node.</xs:documentation>
               <xs:documentation>Starting value for a steady state solution.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="r"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Resistance"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.r">
            <xs:annotation>
               <xs:documentation>Equivalent resistance (RG) of generator. RG is considered for the calculation of all currents, except for the calculation of the peak current ip. Used for short circuit data exchange according to IEC 60909.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="r0"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Resistance"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.r0">
            <xs:annotation>
               <xs:documentation>Zero sequence resistance of the synchronous machine.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ratedS"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ApparentPower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.ratedS">
            <xs:annotation>
               <xs:documentation>Nameplate apparent power rating for the unit.</xs:documentation>
               <xs:documentation>The attribute shall have a positive value.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ratedU"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Voltage"
