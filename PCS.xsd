               <xs:documentation>Negative sequence Thevenin resistance.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="x"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Reactance"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PowerElectronicsConnection.x">
            <xs:annotation>
