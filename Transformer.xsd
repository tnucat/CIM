                     minOccurs="1"
                     maxOccurs="1"
                     type="m:SubGeographicalRegion"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Line.Region">
            <xs:annotation>
               <xs:documentation>The sub-geographical region of the line.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="LinearShuntCompensator"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#LinearShuntCompensator">
      <xs:annotation>
         <xs:documentation>A linear shunt compensator has banks or sections with equal admittance values.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:ShuntCompensator">
            <xs:sequence>
               <xs:element name="b0PerSection"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Susceptance"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#LinearShuntCompensator.b0PerSection">
                  <xs:annotation>
                     <xs:documentation>Zero sequence shunt (charging) susceptance per section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="bPerSection"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Susceptance"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#LinearShuntCompensator.bPerSection">
                  <xs:annotation>
                     <xs:documentation>Positive sequence shunt (charging) susceptance per section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="g0PerSection"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Conductance"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#LinearShuntCompensator.g0PerSection">
                  <xs:annotation>
                     <xs:documentation>Zero sequence shunt (charging) conductance per section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="gPerSection"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Conductance"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#LinearShuntCompensator.gPerSection">
                  <xs:annotation>
                     <xs:documentation>Positive sequence shunt (charging) conductance per section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
