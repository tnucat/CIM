                  <xs:annotation>
                     <xs:documentation>Susceptance per section of the phase if shunt compensator is wye connected.   Susceptance per section phase to phase if shunt compensator is delta connected.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="gPerSection"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Conductance"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#LinearShuntCompensatorPhase.gPerSection">
                  <xs:annotation>
