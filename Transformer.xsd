            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="f">
            <xs:annotation>
               <xs:documentation>Femto 10**-15.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="h">
            <xs:annotation>
               <xs:documentation>Hecto 10**2.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="k">
