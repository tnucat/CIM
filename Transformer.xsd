   <xs:complexType name="LinearShuntCompensatorPhase"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#LinearShuntCompensatorPhase">
      <xs:annotation>
         <xs:documentation>A per phase linear shunt compensator has banks or sections with equal admittance values.</xs:documentation>
      </xs:annotation>
