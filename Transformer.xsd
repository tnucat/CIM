         <xs:extension base="m:Switch">
            <xs:sequence>
               <xs:element name="JumperAction"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:JumperAction"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#Jumper.JumperAction">
                  <xs:annotation>
                     <xs:documentation>Action taken with this jumper.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="JumperAction"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#JumperAction">
      <xs:annotation>
         <xs:documentation>Action on jumper as a switching step.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="Junction"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#Junction">
      <xs:annotation>
         <xs:documentation>A point where one or more conducting equipments are connected with zero resistance.</xs:documentation>
