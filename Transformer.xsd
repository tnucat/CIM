                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#SubGeographicalRegion">
      <xs:annotation>
         <xs:documentation>A subset of a geographical region of a power system network model.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="SvTapStep"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#SvTapStep">
