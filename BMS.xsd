   <xs:complexType name="AirCompressor"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#AirCompressor">
      <xs:annotation>
         <xs:documentation>Combustion turbine air compressor which is an integral part of a compressed air energy storage (CAES) plant.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="airCompressorRating"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#AirCompressor.airCompressorRating">
            <xs:annotation>
               <xs:documentation>Rating of the CAES air compressor.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CAESPlant"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:CAESPlant"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#AirCompressor.CAESPlant">
            <xs:annotation>
               <xs:documentation>An air compressor may be a member of a compressed air energy storage plant.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CombustionTurbine"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:CombustionTurbine"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#AirCompressor.CombustionTurbine">
            <xs:annotation>
               <xs:documentation>A CAES air compressor is driven by combustion turbine.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="BatteryStateKind"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#BatteryStateKind">
      <xs:annotation>
         <xs:documentation>The state of the battery unit.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="charging">
            <xs:annotation>
               <xs:documentation>Stored energy is increasing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="discharging">
            <xs:annotation>
               <xs:documentation>Stored energy is decreasing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="empty">
            <xs:annotation>
               <xs:documentation>Unable to discharge, and not charging.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="full">
            <xs:annotation>
               <xs:documentation>Unable to charge, and not discharging.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="waiting">
            <xs:annotation>
               <xs:documentation>Neither charging nor discharging, but able to do so.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="BatteryStateKind1"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#BatteryStateKind">
      <xs:annotation>
         <xs:documentation>The state of the battery unit.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="charging">
            <xs:annotation>
               <xs:documentation>Stored energy is increasing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="discharging">
            <xs:annotation>
               <xs:documentation>Stored energy is decreasing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="empty">
            <xs:annotation>
               <xs:documentation>Unable to discharge, and not charging.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="full">
            <xs:annotation>
               <xs:documentation>Unable to charge, and not discharging.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="waiting">
            <xs:annotation>
               <xs:documentation>Neither charging nor discharging, but able to do so.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="BatteryUnit"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#BatteryUnit">
      <xs:annotation>
         <xs:documentation>An electrochemical energy storage device.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:PowerElectronicsUnit">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="BatteryUnit1"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#BatteryUnit">
      <xs:annotation>
         <xs:documentation>An electrochemical energy storage device.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:PowerElectronicsUnit">
            <xs:sequence>
               <xs:element name="batteryState"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:BatteryStateKind"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#BatteryUnit.batteryState">
                  <xs:annotation>
                     <xs:documentation>The current state of the battery (charging, full, etc.).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ratedE"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:RealEnergy"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#BatteryUnit.ratedE">
                  <xs:annotation>
                     <xs:documentation>Full energy storage capacity of the battery. The attribute shall be a positive value.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="storedE"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:RealEnergy"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#BatteryUnit.storedE">
                  <xs:annotation>
                     <xs:documentation>Amount of energy currently stored. The attribute shall be a positive value or zero and lower than BatteryUnit.ratedE.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="CAESPlant"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#CAESPlant">
      <xs:annotation>
         <xs:documentation>Compressed air energy storage plant.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="CAESPlant1"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#CAESPlant">
      <xs:annotation>
         <xs:documentation>Compressed air energy storage plant.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="energyStorageCapacity"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:RealEnergy"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CAESPlant.energyStorageCapacity">
            <xs:annotation>
               <xs:documentation>The rated energy storage capacity. The attribute shall be a positive value.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ratedCapacityP"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ActivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CAESPlant.ratedCapacityP">
            <xs:annotation>
               <xs:documentation>The CAES plant's gross rated generating capacity. The attribute shall be a positive value.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AirCompressor"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:AirCompressor"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CAESPlant.AirCompressor">
            <xs:annotation>
               <xs:documentation>An air compressor may be a member of a compressed air energy storage plant.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ThermalGeneratingUnit"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ThermalGeneratingUnit"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CAESPlant.ThermalGeneratingUnit">
            <xs:annotation>
               <xs:documentation>A thermal generating unit may be a member of a compressed air energy storage plant.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CogenerationPlant"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#CogenerationPlant">
      <xs:annotation>
         <xs:documentation>A set of thermal generating units for the production of electrical energy and process steam (usually from the output of the steam turbines). The steam sendout is typically used for industrial purposes or for municipal heating and cooling.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="cogenHPSendoutRating"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CogenerationPlant.cogenHPSendoutRating">
            <xs:annotation>
               <xs:documentation>The high pressure steam sendout.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="cogenHPSteamRating"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CogenerationPlant.cogenHPSteamRating">
            <xs:annotation>
               <xs:documentation>The high pressure steam rating.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="cogenLPSendoutRating"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CogenerationPlant.cogenLPSendoutRating">
            <xs:annotation>
               <xs:documentation>The low pressure steam sendout.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="cogenLPSteamRating"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CogenerationPlant.cogenLPSteamRating">
            <xs:annotation>
               <xs:documentation>The low pressure steam rating.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ratedP"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ActivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CogenerationPlant.ratedP">
            <xs:annotation>
               <xs:documentation>The rated output active power of the cogeneration plant.</xs:documentation>
               <xs:documentation>The attribute shall be a positive value.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SteamSendoutSchedule"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:SteamSendoutSchedule"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CogenerationPlant.SteamSendoutSchedule">
            <xs:annotation>
               <xs:documentation>A cogeneration plant has a steam sendout schedule.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CombinedCyclePlant"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#CombinedCyclePlant">
      <xs:annotation>
         <xs:documentation>A set of combustion turbines and steam turbines where the exhaust heat from the combustion turbines is recovered to make steam for the steam turbines, resulting in greater overall plant efficiency.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="combCyclePlantRating"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ActivePower"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#CombinedCyclePlant.combCyclePlantRating">
            <xs:annotation>
               <xs:documentation>The combined cycle plant's active power output rating.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CombustionTurbine"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#CombustionTurbine">
      <xs:annotation>
         <xs:documentation>A prime mover that is typically fuelled by gas or light oil.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="EmissionAccount"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionAccount">
      <xs:annotation>
         <xs:documentation>Accounts for tracking emissions usage and credits for thermal generating units. A unit may have zero or more emission accounts, and will typically have one for tracking usage and one for tracking credits.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="emissionType"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:EmissionType"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionAccount.emissionType">
            <xs:annotation>
               <xs:documentation>The type of emission, for example sulfur dioxide (SO2). The y1AxisUnits of the curve contains the unit of measure (e.g. kg) and the emissionType is the type of emission (e.g. sulfur dioxide).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="emissionValueSource"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:EmissionValueSource"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionAccount.emissionValueSource">
            <xs:annotation>
               <xs:documentation>The source of the emission value.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ThermalGeneratingUnit"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ThermalGeneratingUnit"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionAccount.ThermalGeneratingUnit">
            <xs:annotation>
               <xs:documentation>A thermal generating unit may have one or more emission allowance accounts.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="EmissionCurve"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionCurve">
      <xs:annotation>
         <xs:documentation>Relationship between the unit's emission rate in units of mass per hour (Y-axis) and output active power (X-axis) for a given type of emission. This curve applies when only one type of fuel is being burned.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="emissionContent"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Emission"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionCurve.emissionContent">
            <xs:annotation>
               <xs:documentation>The emission content per quantity of fuel burned.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="emissionType"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:EmissionType"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionCurve.emissionType">
            <xs:annotation>
               <xs:documentation>The type of emission, which also gives the production rate measurement unit. The y1AxisUnits of the curve contains the unit of measure (e.g. kg) and the emissionType is the type of emission (e.g. sulfur dioxide).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="isNetGrossP"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:boolean"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionCurve.isNetGrossP">
            <xs:annotation>
               <xs:documentation>Flag is set to true when output is expressed in net active power.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ThermalGeneratingUnit"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ThermalGeneratingUnit"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionCurve.ThermalGeneratingUnit">
            <xs:annotation>
               <xs:documentation>A thermal generating unit may have  one or more emission curves.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="EmissionType"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionType">
      <xs:annotation>
         <xs:documentation>The type of emission.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="carbonDioxide">
            <xs:annotation>
               <xs:documentation>Carbon dioxide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="carbonDisulfide">
            <xs:annotation>
               <xs:documentation>Carbon disulfide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="chlorine">
            <xs:annotation>
               <xs:documentation>Chlorine.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="hydrogenSulfide">
            <xs:annotation>
               <xs:documentation>Hydrogen sulfide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="nitrogenOxide">
            <xs:annotation>
               <xs:documentation>Nitrogen oxide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="sulfurDioxide">
            <xs:annotation>
               <xs:documentation>Sulfur dioxide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="EmissionType1"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionType">
      <xs:annotation>
         <xs:documentation>The type of emission.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="carbonDioxide">
            <xs:annotation>
               <xs:documentation>Carbon dioxide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="carbonDisulfide">
            <xs:annotation>
               <xs:documentation>Carbon disulfide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="chlorine">
            <xs:annotation>
               <xs:documentation>Chlorine.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="hydrogenSulfide">
            <xs:annotation>
               <xs:documentation>Hydrogen sulfide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="nitrogenOxide">
            <xs:annotation>
               <xs:documentation>Nitrogen oxide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="sulfurDioxide">
            <xs:annotation>
               <xs:documentation>Sulfur dioxide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="EmissionValueSource"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#EmissionValueSource">
      <xs:annotation>
         <xs:documentation>The source of the emission value.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
